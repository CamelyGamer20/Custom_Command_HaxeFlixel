	<!--_____________Detect_Pc_________________________-->

	<!--Es necesario algunos recursos para que el juego se adapte correctamente y
	para eso verifica tus componentes en este caso en Pc-->

	<define name="PC" if="desktop"/>

	<!--Ram-->

	<!--La ram es necesaria para ejecutar un programa entre mas tengas menos es necesario usar
	por estas especificaciones vamos a Clasificar tu Ordenador-->

	<Pc name="Pc_Gamer" if="Ram_Size_GB >= 32"/>

	<Pc name="Pc_Gamer_Low" if="Ram_Size_GB < 32"/>

	<Pc name="Pc_Comun" if="Ram_Size_GB < 16"/>

	<Pc name="Pc_Office" if="Ram_Size_GB < 8"/>

	<Pc name="Pc_Laptop" if="Ram_Size_GB < 4"/>

	<Pc name="Pc_Potato" if="Ram_Size_GB < 2"/>

	<Pc name="Pc_Potato_Low" if="Ram_Size_GB < 1"/>

	<!--CPU-->

	<!--La CPU es el corazon de la pc este se usa para poner en marcha casi todo trabaja junto a la Ram,GPU
	etc.-->

	<Cpu name="Cpu_Gamer" if="Cpu_Speed >= 3.5"/>

	<Cpu name="Cpu_Gamer_Low" if="Cpu_Speed < 3.5"/>

	<Cpu name="Cpu_Comun" if="Cpu_Speed < 2.5"/>

	<Cpu name="Cpu_Office" if="Cpu_Speed < 1.5"/>

	<Cpu name="Cpu_Laptop" if="Cpu_Speed < 0.5"/>

	<Cpu name="Cpu_Potato" if="Cpu_Speed < 0.25"/>

	<Cpu name="Cpu_Potato_Low" if="Cpu_Speed < 0.1"/>

	<!--GPU-->

	<!--La GPU es necesaria para Cargar archivos de alta Calidad como Sprites de una cantidad muy Grande
	de Pixeles-->

	<Gpu name="Gpu_Gamer" if="Gpu_Speed >= 3.5"/>

	<Gpu name="Gpu_Gamer_Low" if="Gpu_Speed < 3.5"/>

	<Gpu name="Gpu_Comun" if="Gpu_Speed < 2.5"/>

	<Gpu name="Gpu_Office" if="Gpu_Speed < 1.5"/>

	<Gpu name="Gpu_Laptop" if="Gpu_Speed < 0.5"/>

	<Gpu name="Gpu_Potato" if="Gpu_Speed < 0.25"/>

	<Gpu name="Gpu_Potato_Low" if="Gpu_Speed < 0.1"/>

	<!--storage-->

	<!--Sin almacenamiento no se puede descargar un juego necesitamos saber cuanta tienes si tienes algun
	problema descargar algun parche o instalar algun alrchivo que se elimino no nos importa si tu disco
	es SSD o alguno otro solo necesitamos es el Espacio-->

	<Storage name="Storage_Gamer" if="Storage_Size_GB >= 1T"/>

	<Storage name="Storage_Gamer_Low" if="Storage_Size_GB < 1T"/>

	<Storage name="Storage_Comun" if="Storage_Size_GB < 512GB"/>

	<Storage name="Storage_Office" if="Storage_Size_GB < 256GB"/>

	<Storage name="Storage_Laptop" if="Storage_Size_GB < 128GB"/>

	<Storage name="Storage_Potato" if="Storage_Size_GB < 64GB"/>

	<Storage name="Storage_Potato_Low" if="Storage_Size_GB < 32GB"/>

	<!--Screen-->

	<!--Si tienes una pantalla de tamaño muy bajo mas hacer que los Sprites de Ajusten a el mas recomendado
	por tu PC-->

	<Screen name="Screen_Gamer" if="Screen_Size_Inches >= 13"/>

	<Screen name="Screen_Gamer_Low" if="Screen_Size_Inches < 13"/>

	<Screen name="Screen_Comun" if="Screen_Size_Inches < 10"/>

	<Screen name="Screen_Office" if="Screen_Size_Inches < 7"/>

	<Screen name="Screen_Laptop" if="Screen_Size_Inches < 5"/>

	<Screen name="Screen_Potato" if="Screen_Size_Inches < 3"/>

	<Screen name="Screen_Potato_Low" if="Screen_Size_Inches < 1"/>

